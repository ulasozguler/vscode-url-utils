{
	"name": "url-utils",
	"displayName": "URL Utils",
	"description": "Tools for URL stuff.",
	"version": "1.0.1",
	"publisher": "ulasozguler",
	"icon": "icon.png",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ulasozguler/vscode-url-utils"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.urldecode",
		"onCommand:extension.urlencode",
		"onCommand:extension.urlparse",
		"onCommand:extension.urlunparse",
		"onCommand:extension.urlresponse"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.urldecode",
				"title": "URL Decode"
			},
			{
				"command": "extension.urlencode",
				"title": "URL Encode"
			},
			{
				"command": "extension.urlparse",
				"title": "URL Parse"
			},
			{
				"command": "extension.urlunparse",
				"title": "URL Unparse"
			},
			{
				"command": "extension.urlresponse",
				"title": "URL Response"
			}
		],
		"keybindings": [
			{
				"command": "extension.urldecode",
				"key": "alt+u alt+d",
				"mac": "super+u super+d"
			},
			{
				"command": "extension.urlencode",
				"key": "alt+u alt+e",
				"mac": "super+u super+e"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.3.5"
	}
}